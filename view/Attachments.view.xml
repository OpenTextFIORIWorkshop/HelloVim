<mvc:View
	controllerName="HelloVim.view.Attachments"
	xmlns="sap.m" 
	xmlns:form="sap.ui.layout.form"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core">
	<List
    id="AttachmentList"
    headerText="{i18n>ATTACHMENTLIST_HEADER}"
    items="{fakeAttachments>/AttachmentCollection}">
      <items>
        <StandardListItem
        title="{FileName}" />
      </items>
    </List>
</mvc:View>